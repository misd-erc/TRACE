@model IEnumerable<TRACE.Models.CaseEvent>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.EventDatetime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EventDescription)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsUserAction)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CaseEventType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Erccase)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EventDatetime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EventDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsUserAction)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CaseEventType.CaseEventTypeId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Erccase.ErccaseId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CaseEventId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CaseEventId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CaseEventId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>

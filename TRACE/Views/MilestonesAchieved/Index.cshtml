@model IEnumerable<TRACE.Models.MilestonesAchieved>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DatetimeAchieved)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PercentAchieved)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CaseMilestone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Erccase)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DatetimeAchieved)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PercentAchieved)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CaseMilestone.CaseMilestoneId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Erccase.ErccaseId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MilestoneAchievedId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MilestoneAchievedId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MilestoneAchievedId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
